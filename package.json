{
  "name": "maintenance-tracker",
  "version": "1.0.0",
  "description": "Maintenance tracker is a platform that allows users to reach out to maintenance department for repairs and to track the progress of the process",
  "scripts": {
    "babel-node": "nodemon --exec babel-node ./server/src/bin/www.js",
    "build": "babel server/src -s -d server/trans",
    "start:dev": "nodemon --exec && npm run bable-node",
    "start": "node ./server/src/bin/www.js",
    "test": "mocha server/trans/test/",
    "test:local": "npm run build && mocha server/trans/test/",
    "js:lint": "eslint server/src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tutugodfrey/maintenance-tracker.git"
  },
  "keywords": [
    "js",
    "mocha",
    "express"
  ],
  "author": "Tutu Godfrey",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tutugodfrey/maintenance-tracker/issues"
  },
  "homepage": "https://github.com/tutugodfrey/maintenance-tracker#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "codeclimate-test-reporter": "^0.5.0",
    "dotenv-save": "^1.2.0",
    "express": "^4.16.3",
    "morgan": "^1.9.0",
    "multer": "^1.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "coveralls": "^3.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.1.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.17.4"
  }
}
